MODULE user_command_0100 INPUT.

  CASE sy-ucomm.
    WHEN 'EXIT'.
      LEAVE TO SCREEN 0.
    WHEN 'PP'.
      gr_logic->pretty_print( ).
    WHEN 'ATM'.
    gr_logic->add_test_method( ).
  ENDCASE.
ENDMODULE.
